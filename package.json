{
    "name": "@hitachi567/core",
    "version": "0.0.7",
    "description": "basic interfaces, functions and classes for express app",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "publish": "npm publish --access public",
        "rimraf": "./node_modules/rimraf/bin.js",
        "build:clean": "rimraf ./dist",
        "build": "npm run build:clean && tsc",
        "start": "node ./dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-fingerprint": "^1.2.2",
        "helmet": "^4.6.0",
        "jsonwebtoken": "^8.5.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/node": "^16.11.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.3.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.4"
    },
    "keywords": [
        "express"
    ],
    "author": "hitachi567 <abidai790@gmail.com> (https://github.com/hitachi567)",
